<div class="container">
  <div class="row">
    <div class="col-xs-12 col-sm-10 col-sm-offset-1">
      <h1>Create a new cocktail</h1>
      <%= form_for @cocktail do |f| %>
      <%= f.label :name, "Enter a name" %>
      <%= f.text_field :name%>
      <%= f.submit class: 'btn btn-pimary' %>
      <% end%>
    </div>
  </div>
</div>
