<div class="container">
  <div class="row">
    <div class="col-xs-12 col-sm-10 col-sm-offset-1">
      <body>
      <div class = "page_header">
        <h1>Create the new cocktail</h1>
      </div>
      <%= form_for @cocktail do |f| %>
      <%= f.label :name %>
      <%= f.text_field :name%>
      <div class="page_footer">
        <%= f.submit "Create", class: 'btn btn-primary' %>
      </div>
      <% end%>
      </body>
    </div>
  </div>
</div>
